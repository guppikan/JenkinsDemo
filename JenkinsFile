pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello from guppi build stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello from guppi deploy stage'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello from guppi test stage'
            }
        }
        stage('Release') {
            steps {
                echo 'Hello from guppi release stage'
            }
        }
    }
}
